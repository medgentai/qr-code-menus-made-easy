# API URL (base URL without trailing slash)
# Leave empty to auto-detect based on current hostname
# VITE_API_URL=http://localhost:3001
# Note: The backend uses a global prefix of /api/v1 for all endpoints

# Application name
VITE_APP_NAME=ScanServe

# Default pagination limit
VITE_DEFAULT_PAGE_SIZE=10

# Maximum file upload size in bytes (5MB)
VITE_MAX_UPLOAD_SIZE=5242880

# Debug mode
VITE_DEBUG_MODE=true
